    "#自訂Function,一個參數\n",
    "def get_factor(para1):\n",
    "    print(f\"{para1}的因數是:\")\n",
    "    for i in range(1,para1+1):\n",
    "        if para1 % i == 0:\n",
    "         print(i)\n",
    "    print(\"=======結束=======\")"
   ]
